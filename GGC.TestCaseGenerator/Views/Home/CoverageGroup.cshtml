@model GGC.TestCaseGenerator.Models.CoverageGroup



@{
    ViewBag.Title = "Coverage Group";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section scripts
 {

    <script src="~/Scripts/specification-converage.js"></script>
}

<style>

    button {
        margin-right: 15px;
    }
</style>

<body>
    <div class="jumbotron">
        <form method="post">
            <br>
            <label>Add Coverage Group</label>
            <p style="font-size:small;">Please enter the coverage group names of your test specification.</p>
            <div id="CovGroupName">
                <input type="text" id="groupname" name="groupname" style="width:200px" /><input type="button" onclick="AddInputField()" value="Add Group Name" />
            </div><br />
            <input class="btn btn-success" onclick="SubmitGroupName()" type="submit" value="Submit Coverage Group Names" />
            <br>
        </form>
    </div>

</body>
